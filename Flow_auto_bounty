-- Configurações Iniciais
local CoreURL = "https://raw.githubusercontent.com/inkzinho-fofin/Inkzin_hub/refs/heads/main/Flow_Core_auto_bounty.lua?token=GHSAT0AAAAAADS5PTZZO5WOPSG7JP2X23642LNB66A"
local UIURL = "https://raw.githubusercontent.com/inkzinho-fofin/Inkzin_hub/refs/heads/main/Flow_auto_bounty_inter?token=GHSAT0AAAAAADS5PTZYSH4YY47344RFOPOO2LNCAFA"

-- Carregando Repositórios
local Core = loadstring(game:HttpGet(CoreURL))()
local UI = loadstring(game:HttpGet(UIURL))()

-- Criar Interface
local MainPanel = UI:CreateMenu("flow_bounty", "Hatsune_inkyt")

-- Exemplo de uso de funções do Core via LocalScript:
-- Aqui você pode criar Sliders na UI que alteram estes valores:
Core.Settings.TweenSpeed = 100 -- Altera velocidade do teleporte
Core.Settings.SkillDelay = 0.5 -- Altera cooldown das habilidades

print("Sistema Flow Bounty Carregado!")

-- Exemplo de função de caça (Auto-Target)
local function huntPlayer(name)
    local target = game.Players:FindFirstChild(name)
    if target then
        Core:TeleportToPlayer(target)
    end
end
