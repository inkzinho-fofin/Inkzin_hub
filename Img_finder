--[[
    FLOW IMAGE FINDER (LOADER MODULE V2)
    - Processa a ID padr√£o: 135908570815238
]]

local function ProcessarSistema()
    pcall(function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/inkzinho-fofin/Inkzin_hub/refs/heads/main/lista.im"))()
    end)

    local db = getgenv().FlowImagesDatabase or {["Default"] = "135908570815238"}
    local input = getgenv().CustomFruitImg
    local finalID = db["Default"]

    if input == nil or input == "" or input == 0 then
        finalID = db["Default"]
    elseif db[input] then 
        finalID = db[input]
    else
        local numericId = tostring(input):match("%d+")
        finalID = numericId or db["Default"]
    end

    getgenv().FlowImageData = "rbxassetid://" .. finalID
end

ProcessarSistema()
